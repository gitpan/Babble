[%- my_collection = babble_collection || collection.sort -%]
<?xml version="1.0" encoding="[% meta_charset || 'UTF-8' %]"?>
<!-- Generated by Babble -->

<rdf:RDF
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns="http://purl.org/rss/1.0/"
 xmlns:dc="http://purl.org/dc/elements/1.1"
>

<channel rdf:about="[% meta_link %]">
<title><![CDATA[[% meta_title %]]]></title>
<link>[% meta_link %]</link>
<description><![CDATA[[% meta_desc %]]]></description>
<dc:creator>[% meta_owner_email %]</dc:creator>
<dc:date>[% UnixDate (ParseDate (last_update), "%Y-%m-%dT%H:%M:%S+00:00") %]</dc:date>
<items>
 <rdf:Seq>
   [% FOREACH item IN my_collection -%]
   <rdf:li rdf:resource="[% item.id %]" />
   [% END %]
 </rdf:Seq>
</items>
[% IF meta_image.url %]<image rdf:resource="[% meta_image.url %]" />[% END %]
</channel>

[% IF meta_image.url %]
<image rdf:about="[% meta_image.url %]">
  <url>[% meta_image.url %]</url>
  <title>[% meta_image.title %]</title>
  <link>[% meta_image.link %]</link>
  [% IF meta_image.width %]<width>[% meta_image.width %]</width>[% END %]
  [% IF meta_image.height %]<width>[% meta_image.height %]</width>[% END %]
</image>
[% END %]

[% FOREACH item IN my_collection %]
<item rdf:about="[% item.id %]">
 <title><![CDATA[[% item.title %]]]></title>
 <link>[% item.id %]</link>
 <description><![CDATA[[% item.content %]]]></description>
 <dc:creator>[% item.author %]</dc:creator>
 <dc:date>[% item.date_rss %]</dc:date>
 <dc:subject>[% item.subject %]</dc:subject>
</item>
[% END %]

</rdf:RDF>

